generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type OrdersProducts {
  v           Int    @map("__v")
  id          String @map("_id") @db.ObjectId
  description String
  imageUrl    String
  price       String
  quantity    Int
  title       String
  userId      String @db.ObjectId
}

type OrdersUser {
  id    String @map("_id") @db.ObjectId
  email String
}

type UsersCart {
  /// Could not determine type: the field only had null or empty values in the sample set.
  items Json?
}

model orders {
  id       String           @id @default(auto()) @map("_id") @db.ObjectId
  v        Int              @map("__v")
  products OrdersProducts[]
  user     OrdersUser
}

model products {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  v           Int    @map("__v")
  description String
  imageUrl    String
  price       String
  title       String
  userId      String @db.ObjectId
}

model sessions {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  /// Field referred in an index, but found no data to define the type.
  expires Json?

  @@index([expires], map: "expires_1")
}

model users {
  id                   String    @id @default(auto()) @map("_id") @db.ObjectId
  v                    Int       @map("__v")
  cart                 UsersCart
  email                String
  password             String
  /// Could not determine type: the field only had null or empty values in the sample set.
  resetToken           Json?
  /// Could not determine type: the field only had null or empty values in the sample set.
  resetTokenExpiration Json?
}
